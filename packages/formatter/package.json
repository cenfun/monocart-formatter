{
    "name": "monocart-formatter",
    "version": "2.3.0",
    "description": "JS/CSS/HTML/JSON formatter with mapping for both browser (Web Worker) and Node.js (Worker Threads)",
    "main": "dist/monocart-formatter.js",
    "exports": {
        ".": {
            "node": {
                "types": "./lib/index.d.ts",
                "import": "./lib/index.mjs",
                "require": "./lib/index.js",
                "default": "./lib/index.js"
            },
            "types": "./lib/index.d.ts",
            "default": "./dist/monocart-formatter.js"
        },
        "./package.json": "./package.json"
    },
    "types": "./lib/index.d.ts",
    "files": [
        "dist",
        "lib"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/cenfun/monocart-formatter.git"
    },
    "dependencies": {},
    "devDependencies": {}
}
